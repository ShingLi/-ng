/* pages/smile/smile.wxss */
.smile_wrap{
    position: relative;
    width: 100%;
    margin:20rpx 0 30rpx 0;
    
}
.smile_bg{
    position: absolute;
    width: 750rpx;
    height: 535rpx;
    z-index: -1;
    
}
.smile_item{
    padding-top: 20rpx;
    
}
.box{
    margin: 0rpx 45rpx 0 45rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    text-align: center
}
.smile_icon{
    width: 120rpx;
    height: 120rpx;
    background-size: 120rpx 120rpx;
    background-repeat: no-repeat;
}
.smile_icon.smile_y{
    background-image: url('data:image/png;base64,')
}
.smile_icon.smile_x{
    background-image: url('data:image/png;base64,')
}
.smile_icon.smile_g{
    background-image: url('data:image/png;base64,')
}
.time{
    display: block;
    font-size: 18rpx;
    color: rgb(255,255,255);
    margin: 20rpx 0 10rpx 0;

}
.address{
    display: block;
    font-size: 18rpx;
    color: rgb(255,255,255); 
}
.time,.address{
    margin: auto;
}
.bottom{
    margin-top: 20rpx;
    display: flex;
    align-items: center;
}
.describle{
    font-size: 24rpx;
    color: rgb(255,255,255)
}
.duihuan{
    margin-left: 20rpx;
    width: 160rpx;
    height:60rpx;
    font-size: 30rpx;
    line-height: 60rpx;
    display: inline-block;
    border-radius: 30rpx;
}
.duihuan.changeing{
    
    color:rgb(153,153,153);
    background-color: rgb(230,230,230)
}
.duihuan.canchange{
    color:rgb(231,64,62);
    background-color: rgb(252,217,73)
}
.duihuan.changeed{
    color:rgb(51,51,51);
    background-color: rgb(153,153,153)
}